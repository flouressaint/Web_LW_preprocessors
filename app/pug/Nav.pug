block Nav

mixin createNav(title)
    nav.menu
        each key, value in { 'CSS' : 'index' , 'Синтаксис CSS': 'CSS_syntax','Вставка CSS' :'#', 'CSS текст':'#','CSS шрифт':'CSS_font','Блоки':'#'}
            if value == title
                a.active(href= key + ".html")= value
            else
                a(href= key + ".html")= value